                   \ifthenelse{\value{uniquename}>1}
                     {\bibnamedelimd\mkbibbrackets{#2}}
                     {}%
                   \bibnamedelimd}%
  \ifdefvoid{#4}{}{%
    \mkbibnameprefix{#4}\isdot
    \ifprefchar{}{\bibnamedelimc}}%
  \mkbibnamefamily{#1}\isdot%
