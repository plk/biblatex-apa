                            datezeros=false,%
                            eventdate=comp,%
                            labeldate=apalong,%
                            labeldateparts=true,%
                            maxbibnames=20,%
                            maxcitenames=2,%
                            minbibnames=19,%
                            mincitenames=1,%
                            mincrossrefs=999,%
                            maxitems=999,%
                            origdate=apalong,%
                            pagetracker=true,%
                            pluralothers=true,%
                            sortcites=true,%
                            sorting=apa,%
                            uniquelist=minyear,%
                            uniquename=init,%
                            uniqueprimaryauthor=true,%
                            urldate=apalong,%
