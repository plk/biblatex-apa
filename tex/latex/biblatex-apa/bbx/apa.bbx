% (APA 7.09 Example 62) ERIC
% eprint references

\DeclareFieldFormat{eprint:eric}{%
  \printtext{\bibcpstring{retrieved}}%
  \setunit{\addspace}%
  \printtext{\bibstring{from}}\addspace%
  ERIC\addspace database\adddot\addspace%
  \mkbibparens{#1}}

